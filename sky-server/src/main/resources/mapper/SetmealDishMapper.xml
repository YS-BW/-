<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sky.mapper.SetmealDishMapper">
    <insert id="insertSetMealDish">
        insert setmeal_dish (id, dish_id, name, setmeal_id,copies, price) values
            (null, #{dishId}, #{name}, #{setmealId},#{copies},#{price})
    </insert>
    <delete id="deleteSetMealDish">
        delete from setmeal_dish where setmeal_id = #{id}
    </delete>
    <select id="getBySetmealId" resultType="com.sky.entity.SetmealDish">
        select * from setmeal_dish where setmeal_id = #{id}
    </select>
</mapper>